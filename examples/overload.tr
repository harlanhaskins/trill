// RUN: %trill --run %s

type Foo {
  func bar() {
    printf("bar()\n")
  }
  func bar(a: Int) {
    printf("bar(a: %d)\n", a)
  }
  func bar(_ a: Int) {
    printf("bar(_ a: %d)\n", a)
  }
  func bar(_ a: Int, b: Int) {
    printf("bar(_ a: %d, b: %d)", a, b)
  }
}

// overloads the imported Int32 declaration
func putchar(_ char: Int8) {
  putchar(char as Int32)
}

func main() {
  var f = Foo()
  f.bar()
  f.bar(a: 10)
  f.bar(10)
  f.bar(10, b: 100)
}
